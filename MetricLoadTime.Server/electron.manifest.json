{
  "executable": "MetricLoadTime.Server",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "MetricLoadTime",
  "author": "RahulGaur",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.MetricLoadTime.Server.app",
    "productName": "MetricLoadTime",
    "copyright": "Copyright Â© 2024",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "win": {
      "icon": "../../../metricloadtime.client/src/assets/metricloadtime.ico"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}